{
  "name": "brews",
  "version": "1.0.1",
  "description": "A simple boilerplate for Babel, React, EJS, Webpack, and SASS",
  "main": "index.jsx",
  "scripts": {
    "dev": "node server.js",
    "build": "NODE_ENV=production webpack -p --config webpack.config.production.js --progress --profile --colors --optimize-minimize",
    "clean": "rm -rf public"
  },
  "keywords": [
    "Babel",
    "Webpack",
    "React",
    "Boilerplate",
    "SASS",
    "EJS"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jereddanielson/brews"
  },
  "author": "Jered Danielson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jereddanielson/brews/issues"
  },
  "homepage": "https://github.com/jereddanielson/brews",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "clean-webpack-plugin": "^0.1.10",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.8.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  }
}
